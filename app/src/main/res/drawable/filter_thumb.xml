<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:width="@dimen/switch_thumb_size" android:height="@dimen/switch_thumb_size" android:left="1dp" android:top="1dp">
        <shape android:shape="oval">
            <solid android:color="@color/colorPrimary" />
        </shape>
    </item>

    <item android:width="@dimen/switch_thumb_size" android:height="@dimen/switch_thumb_size">
        <shape android:shape="oval">
            <size android:width="@dimen/switch_thumb_size" android:height="@dimen/switch_thumb_size"/>
            <stroke android:color="@android:color/white" android:width="4dp"/>
            <solid android:color="@color/colorPrimary" />
        </shape>
    </item>
</layer-list>